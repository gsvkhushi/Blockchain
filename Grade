pragma solidity >=0.8.2<0.9.0;
contract passfail{
    uint public marks ;
    string public result ;
    function checkpassfail(uint _marks) public{
        marks = _marks ;
        if (marks >90){
            result = "A";
        }else if(marks >=70 ){
            result = "B";
        }else if(marks >= 60){
            result ="C";
        }else if (marks >=40){
            result ="D";
        }else{ 
            result = "F";
        }
    }
}
